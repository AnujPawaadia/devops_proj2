import tkinter as tk
import string


    
about_window = tk.Tk()
about_window.title("About")
about_window.geometry("700x600")
about_label = tk.Label(about_window, text=""" 
                       
                       Welcome to SecurePass
                       
                       """,font=("Times New Roman", 10))
about_label.pack()

about_window.mainloop()
